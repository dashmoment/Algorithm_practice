int mul(int a[], int b[]){
	int i, j; //idx
	na = len(a);
	nb = len(b);
	ma = na/2;
	mb = nb/2;

	int ba[ma];
	int fa[na - ma];
	int bb[mb];
	int fb[nb - mb]

	assign(fa[], a[], ma+1,na);
	assign(ba[], a[], 0,ma);
	assign(fb[], b[], mb+1,nb);
	assign(bb[], b[], 0,nb);

	res (fa,ba,fb,bb, ma, mb)

}

int assign(int *out , int*int, int start, int end);
int res(int fa[], int ba[], int fb[], int bb[], int ma, int mb);

int assign(int *out , int*in, int start, int end){
	int i;
	for(i = 0, i <  end - start, i++){
		out[i] = in[i + start]
	}
}

res (fa,ba,fb,bb, ma, mb){
	total = ma + mb;

	
}
